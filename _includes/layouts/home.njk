---
layout: layouts/base.njk
bodyClass: body-home
---

<header>
  <h1>{{ header }}</h1>
</header>

<section>
  {{ content | safe }}
</section>

<section>
  <h2>Projects</h2>
  <nav>{%- for project in search.pages("project") | reverse -%}
  <a href="{{post.data.url}}">{{ post.data.title }}</a>{%- if loop.index !== loop.length -%}<br />{%- endif -%}
  {%- endfor -%}</nav>
</section>

