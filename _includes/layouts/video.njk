---
layout: layouts/base.njk
bodyClass: body-video
---

<header>
  <h1>{{ title }}</h1>
  
</header>

<section>
  <div class="video-container">
    <iframe width="560" height="315" src="//www.youtube.com/embed/{{ youtube_id }}?start={{ start }}" frameborder="0" allowfullscreen></iframe>
  </div>
  <p>{{ artist }} &middot; {{ video }}</p>
  {{ content | safe }}
  <em>&mdash; {{ date }}</em>
</section>
