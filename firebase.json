{
  "hosting": [ {
      "target": "prod",
      "public": "public",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "redirects": [ {
        "source": "/cal",
        "destination": "https://calendar.hirefrank.com/frank",
        "type": 302
      }, {
        "source": "/calp",
        "destination": "https://calendar.hirefrank.com/frank/30min",
        "type": 302
      } ]
    },
    {
      "target": "staging",
      "public": "public",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ]
    }
  ]
}
