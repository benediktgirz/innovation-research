<ul class="space-y-6 list-none -ml-8">
{{ for p of projects }}
  {{ if p.draft != "true"}}
    <li>{{ p.emoji }} <a href="{{ p.url }}" class="text-on-canvas-dim border-b border-dotted border-on-canvas-dim font-semibold hover:text-accent hover:border-accent">{{ p.title }}</a> - {{ p.summary }}{{ if p.post != null }} <a href="{{ p.url }}">Learn more &#x21A9;</a>{{ /if }}</li>
  {{ /if }}
{{ /for }}
</ul>