<ul class="projects-list">
  {{ for p of projects }}
    {{ if p.draft != "true"}}
      <li>{{ p.emoji }} <a href="{{ p.url }}" class="link-style">{{ p.title }}</a> - {{ p.summary }}{{ if p.post != null }} <a href="{{ p.url }}" class="link-style">Learn more &#x21A9;</a>{{ /if }}</li>
    {{ /if }}
  {{ /for }}
</ul>